{
  "name": "siigo.core.monorepo",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "build": "npx nx affected -t build --parallel=1 --base=develop --head=HEAD",   
    "test": "nx run-many --target=test",
    "lint": "nx run-many --target=lint",
    "e2e": "nx e2e",
    "affected:build": "npx nx affected --target=build --base=develop --head=HEAD",
    "affected:test": "npx nx affected --target=test --base=main --head=HEAD",
    "affected:lint": "npx nx affected --target=lint --base=main --head=HEAD",
    "affected:e2e": "nx affected:e2e",
    "update": "nx migrate latest",
    "dep-graph": "nx dep-graph",
    "prepare": "nx g @nx-dotnet/core:restore"
  },
  "private": true,
  "devDependencies": {
    "@nrwl/js": "16.3.2",
    "@nx-dotnet/core": "^1.22.0",
    "@nx/eslint-plugin": "16.3.2",
    "@nx/jest": "16.3.2",
    "@nx/js": "16.3.2",
    "@nx/linter": "16.3.2",
    "@types/jest": "^29.4.0",
    "@types/node": "18.7.1",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.58.0",
    "eslint": "~8.15.0",
    "eslint-config-prettier": "8.1.0",
    "nx": "16.3.2",
    "nx-cloud": "latest",
    "prettier": "^2.6.2",
    "ts-node": "10.9.1",
    "typescript": "^5.1.3"
  },
  "workspaces": [
    "packages/*"
  ],
  "dependencies": {
    "tslib": "^2.3.0"
  }
}
